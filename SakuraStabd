                    ItemBtn.AnchorPoint = Vector2.new(0.5, 0.5)
                    ItemBtn.BackgroundColor3 = Color3.fromRGB(104, 87, 161)
                    ItemBtn.BorderSizePixel = 0
                    ItemBtn.Position = UDim2.new(0.49000001, 0, 0.5, 0)
